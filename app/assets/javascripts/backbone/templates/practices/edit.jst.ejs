<h1>Edit Practice</h1>
<div class="article">
  <form id="edit-practice" name="practice">
    <div class="field">
      <label for="name"> name:</label>
      <input type="text" name="name" id="name" value="<%= name %>" >
    </div>  

    <div class="field">
      <label for="address"> address:</label>
      <input type="text" name="address" id="address" value="<%= address %>" >
    </div>  

    <div class="field">
      <label for="city"> city:</label>
      <input type="text" name="city" id="city" value="<%= city %>" >
    </div>  

    <div class="field">
      <label for="province"> province:</label>
      <input type="text" name="province" id="province" value="<%= province %>" >
    </div>  

    <div class="field">
      <label for="postal_code"> postal_code:</label>
      <input type="text" name="postal_code" id="postal_code" value="<%= postal_code %>" >
    </div>  

    <div class="field">
      <label for="phone_number"> phone_number:</label>
      <input type="text" name="phone_number" id="phone_number" value="<%= phone_number %>" >
    </div>  

    <div class="field">
      <label for="bio"> bio:</label>
      <input type="text" name="bio" id="bio" value="<%= bio %>" >
    </div>  
  

    <div class="actions">
      <input type="submit" value="Update Practice" />
    </div>
  </form>
  <a href="#/index">Index</a> |
  <a href="#/<%= id %>">Show</td>
</div>




